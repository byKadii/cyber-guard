{
  "manifest_version": 3,
  "name": "Cyber Guard",
  "version": "1.0",
  "description": "Detect malicious websites and warn users",
  "permissions": [
  "tabs",
  "windows",
  "notifications",
  "scripting",
  "webRequest",
  "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["warning.html", "icon.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}